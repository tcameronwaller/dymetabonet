	pmid = {25891169},
	pmcid = {PMC4457580},
	keywords = {Cardiovascular Diseases, Digestive System Diseases, Genome, High-Throughput Screening Assays, Humans, Lung Diseases, Metabolome, Neoplasms, Precision Medicine, Proteome, Systems Biology}
}

